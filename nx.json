{
  "npmScope": "remote-logging",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "server": {
      "tags": ["scope:server"]
    },
    "shared": {
      "tags": ["scope:shared"]
    },
    "frontend": {
      "tags": ["scope:frontend"]
    },
    "frontend-e2e": {
      "tags": ["scope:frontend-e2e"],
      "implicitDependencies": ["frontend"]
    }
  }
}
